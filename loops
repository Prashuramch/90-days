#sum of input digit insingle line like 789=24...
import java.util.*;
public class sod {
	public static void main(String[]args) {
		int n,sum=0;
		Scanner sc=new Scanner(System.in);
		System.out.println("enter mnumber to add:");
		n=sc.nextInt();
		while(n>0) {
			sum= sum+n%10;
			n=n/10;
		}
		System.out.println("added no"+sum);
	}
}
