//odd nos using for loop
import java.util.Scanner;
public class oddnofor {
	public static void main(String[]args) {
		Scanner sc=new Scanner(System.in);
		int i,n;
		System.out.println("enter no's to print odd values");
		n=sc.nextInt();
		for (i=1;i<=n;i++) {
			if (i%2!=0)
			System.out.println(i);
		}
	}

}
